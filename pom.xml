        <spring-boot-starter-jdbc.version>2.7.3</spring-boot-starter-jdbc.version>
        <archunit-junit5.version>1.0.0</archunit-junit5.version>
        <mapstruct.version>1.5.3.Final</mapstruct.version>
        <jackson-databind-nullable.version>0.2.6</jackson-databind-nullable.version>
        <!-- Plugin versions -->
        <maven-clean-plugin.version>3.1.0</maven-clean-plugin.version>
        <maven-enforcer-plugin.version>3.1.0</maven-enforcer-plugin.version>
        <maven-compiler-plugin.version>3.8.1</maven-compiler-plugin.version>
        <maven-javadoc-plugin.version>3.2.0</maven-javadoc-plugin.version>
        <maven-failsafe-plugin.version>3.0.0-M5</maven-failsafe-plugin.version>
        <maven-idea-plugin.version>2.2.1</maven-idea-plugin.version>
        <maven-resources-plugin.version>3.2.0</maven-resources-plugin.version>
        <maven-surefire-plugin.version>3.0.0-M5</maven-surefire-plugin.version>
        <maven-war-plugin.version>3.3.1</maven-war-plugin.version>
        <maven-checkstyle.version>3.1.2</maven-checkstyle.version>
        <checkstyle.version>8.41.1</checkstyle.version>
        <spring-nohttp-checkstyle.version>0.0.5.RELEASE</spring-nohttp-checkstyle.version>
        <spring-data.version>2.4.0</spring-data.version>
        <git-commit-id-plugin.version>4.0.4</git-commit-id-plugin.version>
        <jacoco-maven-plugin.version>0.8.8</jacoco-maven-plugin.version>
        <jib-maven-plugin.version>2.8.0</jib-maven-plugin.version>
        <lifecycle-mapping.version>1.0.0</lifecycle-mapping.version>
        <openapi-generator-maven-plugin.version>6.3.0</openapi-generator-maven-plugin.version>
        <properties-maven-plugin.version>1.0.0</properties-maven-plugin.version>
        <sonar-maven-plugin.version>3.8.0.2131</sonar-maven-plugin.version>
        <lombok.version>1.18.24</lombok.version>
        <postgresql.version>42.5.1</postgresql.version>
        <flyway-core.version>8.5.13</flyway-core.version>
        <io-springfox.version>3.0.0</io-springfox.version>
        <jakarta-validation.version>2.0.2</jakarta-validation.version>
        <io-swagger-core.version>2.2.10</io-swagger-core.version>
        <!-- jhipster-needle-maven-property -->
        <hibernate-envers.version>5.6.5.Final</hibernate-envers.version>
        <hibernate-types-52.version>2.12.1</hibernate-types-52.version>
        <querydsl.version>5.0.0</querydsl.version>
        <lombok-mapstruct-binding.version>0.2.0</lombok-mapstruct-binding.version>
        <commons-validator.version>1.7</commons-validator.version>

        <micrometer.version>1.11.0</micrometer.version>
        <problem-spring-web.version>0.27.0</problem-spring-web.version>
        <snowflake-id-generator.version>2.5.0</snowflake-id-generator.version>
        <commons-lang3.version>3.12.0</commons-lang3.version>
        <commons-beanutils.version>1.9.4</commons-beanutils.version>
        <commons-collections4.version>4.4</commons-collections4.version>
    </properties>

